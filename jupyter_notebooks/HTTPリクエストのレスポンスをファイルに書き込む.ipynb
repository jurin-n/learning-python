   "source": [
    "import requests\n",
    "# GitHubAPIにリクエスト\n",
    "res = requests.get('https://api.github.com')\n",
    "res.raise_for_status()\n",
    "\n",
    "# ファイルオープン\n",
    "play_file = open('GitHubAPI.txt', 'wb')\n",
    "\n",
    "# レスポンス書き込む\n",
    "for chunk in res.iter_content(100*1024):\n",
    "         play_file.write(chunk)\n",
    "\n",
    "# ファイルクローズ        \n",
    "play_file.close()\n"
   ]
  },
